{
  "version": "0.2",
  "language": "en",
  "words": [],
  "dictionaries": ["project-words"],
  "dictionaryDefinitions": [
    {
      "name": "project-words",
      "path": "./.cspell/project-words.txt",
      "addWords": true
    }
  ],
  "ignorePaths": [
    "node_modules/**",
    "exports/**",
    "source-materials/**",
    "*.pdf",
    "*.docx"
  ],
  "ignoreRegExpList": [
    "\\[\\[.*?\\]\\]",
    "[\\uAC00-\\uD7AF]+",
    "[\\u1100-\\u11FF]+",
    "[\\u3130-\\u318F]+"
  ]
}
